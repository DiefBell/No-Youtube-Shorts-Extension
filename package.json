{
	"name": "no-youtube-shorts",
	"version": "1.0.0",
	"description": "A Google Chrome extension to hide YouTube shorts",
  "scripts": {
    "build": "yarn lint && webpack --config webpack.prod.js",
    "dev": "yarn lint:fix && webpack --config webpack.dev.js",
    "watch": "yarn lint:fix && webpack -w --config webpack.dev.js",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix"
  },
  "author": "Joe Martella",
  "license": "MIT",
  "devDependencies": {
    "@types/chrome": "^0.0.208",
    "@types/jsdom": "^16.2.14",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "@types/react-fontawesome": "^1.6.5",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.17.0",
    "css-loader": "^6.7.1",
    "eslint": "^8.12.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^16.1.4",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "node-sass": "^7.0.1",
    "sass-loader": "^12.6.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.2.8",
    "typescript": "^4.6.3",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free-solid": "^5.0.13",
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/react-fontawesome": "^0.1.18",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-query": "^3.34.17"
  }
}
